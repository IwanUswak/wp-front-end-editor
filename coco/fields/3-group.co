class FrontEndEditor.fieldTypes.group extends FrontEndEditor.fieldTypes.base
	editors: [],

	ajax_get: ->
		data = {
			callback : 'get',
			action   : 'front-end-editor',
			group    : true,
			nonce    : FrontEndEditor.data.nonce,
			data     : for editor of @editors
				editor.form_create()
				editor.data
		}

		jQuery.post FrontEndEditor.data.ajax_url, data, this.~ajax_get_handler, 'json'

	ajax_get_handler: (response) ->
		for editor, i of @editors
			editor.ajax_get_handler(response[i])
